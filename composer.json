{
  "name": "yiqiniu/think-extend",
  "description": "think php 6.0 extend library",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "gjianbo",
      "email": "gjianbo@qq.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "yiqiniu/framework": "^6.0.*.dev",
    "guzzlehttp/guzzle": "~6.0",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-iconv": "*",
    "ext-fileinfo": "*",
    "ext-dom": "*",
    "gouguoyin/easyhttp": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "yiqiniu\\": "src"
    },
    "files": [
      "src/const.php",
      "src/helpers.php"
    ]
  },
  "extra": {
    "think": {
      "services": [
        "yiqiniu\\Service"
      ]
    },
    "config": {
      "extend": "src/config/extend.php"
    }
  },
  "minimum-stability": "dev"
}
